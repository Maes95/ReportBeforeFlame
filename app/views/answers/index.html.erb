<p id="notice"><%= notice %></p>

<h1>Listing Answers</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Type</th>
      <th>Stakeholder</th>
      <th>Report</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @answers.each do |answer| %>
      <tr>
        <td><%= answer.content %></td>
        <td><%= answer.type %></td>
        <td><%= answer.stakeholder_id %></td>
        <td><%= answer.report_id %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
