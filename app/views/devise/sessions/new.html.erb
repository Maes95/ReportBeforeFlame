<% content_for :new_session do %>
<div class="col-md-2 col-xs-0"></div>
<div class="main_pane col-md-4 col-xs-12">
<h2>Log in </h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true,  class:"form-control", placeholder:"Enter your email" %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off",  class:"form-control", placeholder:"Enter your password" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <button  class="btn btn-info" type="submit">Sign in</button>
    <a class="btn " href="/stakeholders/sign_up" role="button">Sign up</a>
  </div>
  <!--<button class="btn btn-default" href="javascript:history.back()">Back</button>-->
<% end %>
</div>
<% end %>
