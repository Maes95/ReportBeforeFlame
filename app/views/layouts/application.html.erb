<!DOCTYPE html>
<html>
<head>
  <title>ReportBeforeFlame</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>


<% if current_stakeholder %>
<div class="alert alert-info" role="alert">
  Welcome <%= current_stakeholder.name %> - <%= link_to 'Logout', destroy_stakeholder_session_path, method: :delete %>
</div>
  <div class="report">
    <%= yield %>
  </div>
<% else %>
        <%= yield :new_session %>
        <%= yield :home %>
<% end %>

</body>
</html>
