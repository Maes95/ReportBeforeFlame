<p id="notice"><%= notice %></p>

<h1>Listing Stakeholders</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Lastname</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @stakeholders.each do |stakeholder| %>
      <tr>
        <td><%= link_to stakeholder.name, stakeholder %></td>
        <td><%= stakeholder.lastname %></td>
        <td><%= stakeholder.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
